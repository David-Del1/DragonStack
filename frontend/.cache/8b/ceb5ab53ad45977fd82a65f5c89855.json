"original":{"line":48,"column":13}},{"generated":{"line":95,"column":276},"source":"components/Generation.jsx","original":{"line":48,"column":8}},{"generated":{"line":95,"column":277},"source":"components/Generation.jsx","original":{"line":46,"column":6}},{"generated":{"line":95,"column":278},"source":"components/Generation.jsx","original":{"line":45,"column":4}},{"generated":{"line":96,"column":0},"source":"components/Generation.jsx","original":{"line":51,"column":3}},{"name":"Component","generated":{"line":100,"column":2},"source":"components/Generation.jsx","original":{"line":6,"column":32}},{"name":"Generation","generated":{"line":103,"column":15},"source":"components/Generation.jsx","original":{"line":54,"column":15}}],"sources":{"components/Generation.jsx":"import React, { Component } from 'react'\n\nconst DEFAULT_GENERATION = { generationId: '', expiration: '' };\nconst MINIMUM_DELAY = 3000;\n\nexport class Generation extends Component {\n  state = { generation: DEFAULT_GENERATION };\n\n  timer = null;\n\n  componentDidMount() {\n    this.fetchNextGeneration();\n  }\n\n  componentWillUnmount() {\n    clearTimeout(this.timer);\n  }\n  \n  \n\n  fetchGeneration = () => {\n    fetch('http://localhost:8000/generation')\n      .then(res => res.json())\n      .then(json => { \n        console.log('json',json)\n\n        this.setState({ generation: json.generation });\n      })\n      .catch(err => console.error('error ',err))\n  }\n\n  fetchNextGeneration = () => {\n    this.fetchGeneration();\n\n    let delay = new Date(this.state.generation.expiration).getTime() - new Date().getTime();\n\n    if(delay < MINIMUM_DELAY) delay = MINIMUM_DELAY;\n\n    this.timer = setTimeout(() => this.fetchNextGeneration(), delay);\n  }\n\n  render() {\n    const { generation } = this.state;\n\n    return (\n      <div>\n        <h3>Generation {generation.generationId}. Expires on:</h3>\n        <h4>{new Date(generation.expiration).toString()}</h4>\n      </div>\n    )\n  }\n}\n\nexport default Generation;\n"},"lineCount":null}},"error":null,"hash":"ee71beea6a59a8f4b920956876f7ecf0","cacheData":{"env":{}}}